@model Reinforced.Lattice.DebugSink.Models.AdditionalSearchData
<div class="row">
    <div class="col-md-6">
        <h3>Sample search form</h3>
        <div class="form-horizontal">
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">@Html.LabelFor(c=>c.MinimumCost)</label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(c => c.MinimumCost, new { @class = "form-control" })
                </div>
            </div>
            <div class="form-group">
                <label for="inputPassword3" class="col-sm-2 control-label">@Html.LabelFor(c=>c.GroupNamePart)</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control _part" id="inputPassword3" placeholder="Group Name Part...">
                </div>
            </div>
            <div class="form-group">
                <label for="inputPassword3" class="col-sm-2 control-label">@Html.LabelFor(c => c.ICloudLock)</label>
                <div class="col-sm-10">
                    @Html.DropDownListFor(c => c.ICloudLock, Model.ValuesForIcloudlock, new {@class = "form-control"})
                </div>
            </div>
            <div class="form-group">
                <label for="inputPassword3" class="col-sm-2 control-label">@Html.LabelFor(c => c.TypesList)</label>
                <div class="col-sm-10">
                    @Html.ListBoxFor(c => c.TypesList, Model.Types, new { @class = "form-control" })
                </div>
            </div>
        </div>
    </div>
</div>
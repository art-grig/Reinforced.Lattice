{
  "version": "1.0.0-*",
  "title": "Reinforced.Lattice",
  "authors": [ "Pavel B. Novikov" ],
  "description": "Reinforced.Lattice is ultimate remote data representation framework for .NET with extended templating engine",
  "packOptions": {
    "projectUrl": "https://github.com/reinforced/Reinforced.Lattice",
    "tags": [ "datagrid", "asp.net", "mvc", "virtualizing", "search", "order", "filter" ],
    "licenseUrl": "https://raw.githubusercontent.com/reinforced/Reinforced.Lattice/master/LICENSE.md",
    "iconUrl": "https://raw.githubusercontent.com/reinforced/Reinforced.Lattice/master/icon.png",
    "repository": {
      "type": "git",
      "url": "https://github.com/reinforced/Reinforced.Lattice"
    },
    "files": [ "*.dll", "*.xml" ],
    "owners": [ "Pavel B. Novikov" ]
  },
  "runtimes": {
    "win": {}
  },
  "frameworks": {
    "net40": {
      "buildOptions": {
        "copyToOutput": "..\\build",
        "define": [ "NET40" ]
      },
      "frameworkAssemblies": {
        "System.Data": "",
        "System.Numerics": "",
        "System.Runtime.Serialization": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "System.ComponentModel.DataAnnotations": ""
      },
      "dependencies": {
        "Newtonsoft.Json": "7.0.1"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Data": "",
        "System.Numerics": "",
        "System.Runtime.Serialization": "",
        "System.Xml": "",
        "System.Xml.Linq": "",
        "System.ComponentModel": ""
      },
      "dependencies": {
        "Newtonsoft.Json": "7.0.1"
      },
      "buildOptions": {
        "define": [ "NET45", "ASYNC", "NETCORE" ],
        "copyToOutput": "..\\build"
      }
    },
    "netstandard1.6": {
      "buildOptions": {
        "define": [ "NET45", "ASYNC", "NETCORE" ],
        "copyToOutput": "..\\build"
      },
      "imports": "dnxcore50",
      "dependencies": {
        "Newtonsoft.Json": "7.0.1",
        "System.Collections": "4.3.0",
        "System.ComponentModel.TypeConverter": "4.3.0",
        "System.Linq": "4.3.0",
        "System.Linq.Expressions": "4.3.0",
        "System.Linq.Queryable": "4.3.0",
        "System.Reflection": "4.3.0",
        "System.Reflection.Extensions": "4.3.0",
        "System.Console": "4.3.0",
        "System.Collections.Concurrent": "4.3.0",
        "System.Runtime.Extensions": "4.3.0",
        "System.Threading": "4.3.0"
      }
    }
  }
}
